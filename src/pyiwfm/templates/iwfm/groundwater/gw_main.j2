C             {{ generation_time }}
C
C*******************************************************************************
C                   Input and Output Data File Names
C
C----------------------------------------------------------------------------------------
C         VALUE                                      DESCRIPTION
C----------------------------------------------------------------------------------------
          {{ bc_file | pad_right(40) }} / BCFL
          {{ td_file | pad_right(40) }} / TDFL
          {{ pump_file | pad_right(40) }} / PUMPFL
          {{ subs_file | pad_right(40) }} / SUBSFL
          {{ overwrite_file | pad_right(40) }} / OVRWRTFL
          {{ length_factor | pad_right(40) }} / FACTLTOU
          {{ length_unit | pad_right(40) }} / UNITLTOU
          {{ volume_factor | pad_right(40) }} / FACTVLOU
          {{ volume_unit | pad_right(40) }} / UNITVLOU
          {{ velocity_factor | pad_right(40) }} / FACTVROU
          {{ velocity_unit | pad_right(40) }} / UNITVROU
          {{ velocity_file | pad_right(40) }} / VELOUTFL
          {{ vertical_flow_file | pad_right(40) }} / VFLOWOUTFL
          {{ head_all_file | pad_right(40) }} / GWALLOUTFL
          {{ tecplot_file | pad_right(40) }} / HTPOUTFL
          {{ vtk_file | pad_right(40) }} / VTPOUTFL
          {{ budget_file | pad_right(40) }} / GWBUDFL
          {{ zbudget_file | pad_right(40) }} / ZBUDFL
          {{ final_heads_file | pad_right(40) }} / FNGWFL
          {{ head_output_flag }}                                          / IHTPFLAG
C*******************************************************************************
C                             Debugging
C-------------------------------------------------------------------------------
    {{ debug_flag }}                           / KDEB
C*******************************************************************************
C                       Groundwater Hydrograph Output Data
C-------------------------------------------------------------------------------
    {{ n_hydrographs }}                           / NOUTH
    {{ hydrograph_xy_factor }}                         / FACTXY
    {{ hydrograph_file | pad_right(40) }} / GWHYDOUTFL
{% if hydrograph_locations %}
C-------------------------------------------------------------------------------
C   ID    HYDTYP  IOUTHL    X         Y        IOUTH  NAME
C-------------------------------------------------------------------------------
{% for loc in hydrograph_locations %}
    {{ loc.node_id | fortran_int(5) }}  {{ loc.layer | fortran_int(5) }}  {{ loc.node_id | fortran_int(5) }}  {{ loc.x | fortran_float(12, 2) }}  {{ loc.y | fortran_float(12, 2) }}  0  {{ loc.name }}
{% endfor %}
{% endif %}
{% if face_flow_specs %}
C*******************************************************************************
C                       Face Flow Output Data
C-------------------------------------------------------------------------------
    {{ n_face_flows }}                           / NOUTF
    {{ face_flow_file | pad_right(40) }} / FACEFLOWFL
{% for ff in face_flow_specs %}
    {{ ff }}
{% endfor %}
{% endif %}
C*******************************************************************************
C                       Aquifer Parameter Data
C
C   NGROUP    ; Number of parametric grid groups for aquifer parameters
C               (0 = non-parametric input)
C-------------------------------------------------------------------------------
    {{ n_param_groups }}                           / NGROUP
C*******************************************************************************
C                    Non-Parametric Aquifer Parameters
C
C   FACTHK    ; Conversion factor for Kh
C   FACTKV    ; Conversion factor for Kv
C   FACTSS    ; Conversion factor for Ss
C   FACTSY    ; Conversion factor for Sy
C   FACTKAQ   ; Conversion factor for aquitard Kv
C   FACTKHA   ; Conversion factor for anomaly Kh (leave 0 if none)
C-------------------------------------------------------------------------------
    {{ fact_kh }}                         / FACTHK
    {{ fact_kv }}                         / FACTKV
    {{ fact_ss }}                         / FACTSS
    {{ fact_sy }}                         / FACTSY
    {{ fact_kaq }}                         / FACTKAQ
    {{ fact_kha }}                         / FACTKHA
C-------------------------------------------------------------------------------
C   Time units for Kh, aquitard Kv, and Kv
C-------------------------------------------------------------------------------
    {{ time_unit_kh | pad_right(28) }}/ TUNITKH
    {{ time_unit_kaq | pad_right(28) }}/ TUNITV
    {{ time_unit_kv | pad_right(28) }}/ TUNITL
C*******************************************************************************
C   Node aquifer parameters:
C
C   ID      ; Node ID
C   For each layer:
C     KH    ; Horizontal hydraulic conductivity [L/T]
C     KV    ; Vertical hydraulic conductivity [L/T]
C     SS    ; Specific storage [1/L]
C     SY    ; Specific yield [-]
C     KAQ   ; Aquitard vertical conductivity [L/T]
C
C-------------------------------------------------------------------------------

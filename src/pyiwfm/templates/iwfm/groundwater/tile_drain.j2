C *** DO NOT DELETE ABOVE LINE ***
C*******************************************************************************
C                  TILE DRAINS DATA FILE
C
C             Generated by pyiwfm
C             {{ generation_time }}
C*******************************************************************************
C
C   NTD     ; Number of tile drains
C   FACTH   ; Conversion factor for tile drain elevations
C   FACTCDC ; Conversion factor for tile drain conductances
C   TUNITDR ; Time unit for conductance
C
C-------------------------------------------------------------------------------
    {{ n_drains | fortran_int(10) }}                         / NTD
    {{ td_elev_factor }}                         / FACTH
    {{ td_cond_factor }}                         / FACTCDC
    {{ td_time_unit }}                           / TUNITDR
C-------------------------------------------------------------------------------
C   Tile Drain Parameters
C
C   IDDR   NODEDR    ELEVDR          CDCDR   TYPDST  DST
C-------------------------------------------------------------------------------
{% for drain in drains %}
    {{ drain.id | fortran_int(5) }}    {{ drain.gw_node | fortran_int(5) }}    {{ drain.elevation | fortran_float(10, 3) }}    {{ drain.conductance | fortran_float(10, 6) }}    {{ drain.dest_type | fortran_int(2) }}    {{ drain.dest_id | fortran_int(5) }}
{% endfor %}
C*******************************************************************************
C                  Subsurface Irrigation Parameters
C-------------------------------------------------------------------------------
    {{ n_subirrig | fortran_int(10) }}                         / NSI
    {{ si_elev_factor }}                         / FACTHSI
    {{ si_cond_factor }}                         / FACTCDCSI
    {{ si_time_unit }}                           / TUNITSI
{% for si in sub_irrigation %}
    {{ si }}
{% endfor %}
C*******************************************************************************
C                  Tile Drain/Subsurface Irrigation Hydrograph Output
C-------------------------------------------------------------------------------
    {{ n_td_hydro | fortran_int(10) }}                         / NOUTHTD
    {{ td_output_file | pad_right(40) }} / TDOUTFL
C-------------------------------------------------------------------------------
{% for loc in td_hydro_locations %}
    {{ loc }}
{% endfor %}

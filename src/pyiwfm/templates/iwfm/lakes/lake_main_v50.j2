C*******************************************************************************
C                          Lake Data File Names
C
C----------------------------------------------------------------------------------------
C         VALUE                                      DESCRIPTION
C----------------------------------------------------------------------------------------
          {{ budget_file | pad_right(40) }} / LKBUDFL
          {{ final_elev_file | pad_right(40) }} / FNLKELVFL
C*******************************************************************************
C                           Lake Parameters
C
C-------------------------------------------------------------------------------
C   VALUE                 DESCRIPTION
C-------------------------------------------------------------------------------
    {{ factk }}                   / FACTK
    {{ tunitk | pad_right(22) }}/ TUNITK
    {{ factl }}                   / FACTL
C*******************************************************************************
C                      Lake Outflow Rating Tables
C
C-------------------------------------------------------------------------------
C   VALUE                 DESCRIPTION
C-------------------------------------------------------------------------------
    {{ factlkl }}                   / FACTLKL (elevation factor)
    {{ factlkq }}                   / FACTLKQ (flow factor)
    {{ tunitlkq | pad_right(22) }}/ TUNITLKQ
C-------------------------------------------------------------------------------
C   IL   CLAKE   DLAKE   ICETLK  ICPCPLK  NPOINTS  NAMELK
C-------------------------------------------------------------------------------
{% for lake in lake_data %}
    {{ lake.id | fortran_int(5) }}     {{ lake.conductivity | fortran_float(4, 1) }}      {{ lake.thickness | fortran_float(3, 1) }}          {{ lake.icetlk }}        {{ lake.icpcplk }}   {{ lake.n_rating_points | fortran_int(5) }}       {{ lake.name }}
{% if lake.rating_points %}
C   Rating table: Elevation  Flow
{% for pt in lake.rating_points %}
    {{ pt.elevation | fortran_float(14, 4) }}  {{ pt.flow | fortran_float(14, 4) }}
{% endfor %}
{% endif %}
{% endfor %}
